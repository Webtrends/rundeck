debug=<%= @debug %>
datacenter=<%= @datacenter %>
pod=<%= @pod %>
kafka.host=<%= @kafka %>
geo.host=<%= @netacuity %>
cs.url=<%= @configservice %>/dcsid/streaming

# Kafka consumer parameters
kafka.zookeeper.quorum=<%= @kafka_zookeeper_quorum %>
kafka.zookeeper.timeout.milliseconds=<%= @kafka_zookeeper_timeout_milliseconds %>
kafka.consumer.topic=<%= @kafka_consumer_topic %>
kafka.consumer.group.id=<%= @kafka_consumer_group_id %>

# Hbase and zookeeper parameters
hbase.pool.size=80
hbase.zookeeper.quorum=<%= @zookeeper_quorum  %><%= @zookeeper_quorum_path %>
hbase.zookeeper.property.clientPort=<%=@zookeeper_clientport%>

# Topology should be streaming or realtime
topology=streaming-topology

# Streaming parameters, should have blank values in streaming
streaming.topology.in.session.bolt.count=<%= @streaming_topology_in_session_bolt_count %>
streaming.topology.zmq.emitter.bolt.count=<%= @streaming_topology_zmq_emitter_bolt_count %>
streaming.topology.zmq.emitter.znodeRoot=/StreamingZMQ
streaming.topology.validation.bolt.count=<%= @streaming_topology_validation_bolt_count %>
streaming.topology.augmentation.bolt.count=<%= @streaming_topology_augmentation_bolt_count %>

# Zookeeper connection info (use this or audit.broker.list)
audit.zk.connect=<%= @zookeeper_pairs %>
# The kafka producer type (defaults to async)
audit.producer.type=async
# The time span (in minutes) between each audit flush (defaults to 10)
audit.bucket.timespan=<%= @audit_bucket_timespan %>
# The topic used for auditing (defaults to stream-audit)
audit.topic=<%= @audit_topic %>

# This is the base url for the CAM service
webtrends.cam.baseUrl = <%=@cam_url%>
# This is the url to the REST service for getting product provisioning
webtrends.cam.provisioningDetail=%%baseUrl%%/cam/api/account/%%accountId%%/product/%%productId%%
