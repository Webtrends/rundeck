<?xml version="1.0" encoding="utf-8"?>
<appSettings>
    <add key="webpages:Version" value="2.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />

    <!-- ELMAH -->
    <add key="elmah.mvc.disableHandler" value="false" />
    <add key="elmah.mvc.requiresAuthentication" value="false" />
    <add key="elmah.mvc.allowedRoles" value="*" />
    <add key="elmah.mvc.route" value="secure/admin/errors" />

    <!-- streaming locations -->
    <add key="auth.url" value="<%= @auth_url %>"/>
    <add key="auth.url.base" value="<%= @auth_url_base %>"/>
    <add key="cam.url" value="<%= @cam_url %>"/>
    <add key="cam.url.base" value="<%= @cam_url_base %>"/>
    <add key="sapi.url" value="<%= @sapi_url %>"/>
    <add key="help.url" value="<%= @help_url %>"/>
    <add key="account.url" value="<%= @account_url %>"/>
    <add key="streams.url" value="<%= @streams_url %>"/>
    <add key="sms.url" value="<%= @sms_url %>" />
    <add key="stream.client.id" value="<%= @stream_client_id %>"/>
    <add key="stream.client.secret" value="<%= @stream_client_secret %>"/>
    <add key="appKey" value="streams"/>
    <add key="thumbnailurl" value="<%= @thumbnail_service_url %>" />
</appSettings>

