<?xml version="1.0" encoding="utf-8"?>
<appSettings>
    <add key="webpages:Version" value="2.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  
    <!-- ELMAH -->
    <add key="elmah.mvc.disableHandler" value="false" />
    <add key="elmah.mvc.requiresAuthentication" value="false" />
    <add key="elmah.mvc.allowedRoles" value="*" />
    <add key="elmah.mvc.route" value="secure/admin/errors" />

    <!-- Licensing -->
    <add key="aps.url" value="<%= @aps_url %>"/>
    <add key="aps.url.base" value=""/>
    <add key="mc.url" value="https://mc"/>

    <!-- streaming locations -->
    <add key="cam.url" value="<%= @cam_url %>"/>
    <add key="cam.url.base" value="<%= @cam_url_base %>"/>
    <add key="config.url" value="<%= @config_url%>"/>
    <add key="ad.network" value="<%= @ad_network %>"/>

    <!-- User Access GruopNames -->
    <add key="ad.group.admin" value="*"/>
    <add key="ad.group.user" value="*"/>
    <add key="reauth.minutes" value="90"/>

</appSettings>

