<?xml version="1.0" encoding="utf-8"?>
<appSettings>
    <add key="webpages:Version" value="2.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />

    <!-- ELMAH -->
    <add key="elmah.mvc.disableHandler" value="false" />
    <add key="elmah.mvc.requiresAuthentication" value="false" />
    <add key="elmah.mvc.allowedRoles" value="*" />
    <add key="elmah.mvc.route" value="secure/admin/errors" />

    <!-- Licensing -->
    <add key="aps.url" value="<%= @aps_url %>"/>
    <add key="aps.url.base" value="<%= @aps_url_base %>"/>
    <add key="mc.url" value="<%= @management_console_url %>"/>

    <!-- streaming locations -->
    <add key="cam.url" value="<%= @cam_url %>"/>
    <add key="cam.url.base" value="<%= @cam_url_base %>"/>
    <add key="config.url" value="<%= @config_url%>"/>
    <add key="ad.network" value="<%= @portmgr_domain %>"/>

    <!-- User Access GroupNames -->
    <add key="ad.group.admin" value="<%= @portmgr_group_admin%>"/>
    <add key="ad.group.user" value="<%= @portmgr_group_user%>"/>
    <add key="reauth.minutes" value="90"/>

    <!-- New Account Notification -->
    <add key="injected.user" value="<%= @portmgr_injected_user%>"/>

    <!-- Fulfillment Email -->
    <add key="fulfillment.dl" value="<%= @portmgr_fulfillment_dl%>"/>
    <add key="support.dl" value="<%= @portmgr_support_dl%>"/>
    <add key="email.from" value="<%= @portmgr_email_from%>"/>
    <add key="email.imagepath" value="<%= @auth_url %>/content/images/email"/>
    <add key="streams.url" value="<%= @streams_ui_url %>" />

    <!-- Action Center overrides -->
    <add key="actioncenter.availablecategories" value="{&quot;categories&quot;:[{&quot;name&quot;:&quot;EmailRemarketing&quot;,&quot;max_actions&quot;:25,&quot;max_delivery_events&quot;:1500000,&quot;destination&quot;:[{&quot;name&quot;:&quot;Responsys&quot;},{&quot;name&quot;:&quot;ExactTarget&quot;},{&quot;name&quot;:&quot;Silverpop&quot;},{&quot;name&quot;:&quot;Webtrends&quot;}]}]}" />

</appSettings>
