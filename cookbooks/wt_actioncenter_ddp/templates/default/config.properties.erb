# Kafka topic base to push to  the account id or some other data should be
# tacked on the end
# dynamically so that the data is seperate
kafka.topic=<%= @kafka_topic %>

# Kafka consumer parameters
kafka.consumer.group.id=action-center-integration-example
kafka.auto.offset.reset=smallest
kafka.auto.commit.enable=true

configData.host=<%=@config_host%>
secureConfigData.host=<%= @secure_config_host %>
secureConfigData.port=<%= @secure_config_port %>
