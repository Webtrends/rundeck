# are we monitoring at all
monitoring.enabled = <%= @wt_monitoring['monitoring_enabled'] %>
# should we publish to jmx
monitoring.jmx.enabled = <%= @wt_monitoring['jmx_enabled'] %>
# the prefix to append metrics being sent to graphite
monitoring.metric.prefix = <%= @wt_monitoring['metric_prefix'] %>

# Are health checks enabled
healthcheck.enabled=<%= @wt_monitoring['healthcheck_enabled'] %>
# This is for future use for having health checks bound to an alternative port
healthcheck.port=<%= @wt_monitoring['healthcheck_port'] %>
