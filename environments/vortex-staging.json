                "time.webtrends.corp"
            ]
        }, 
        "postfix": {
            "relayhost": "pdxengmta01.staging.dmz"
        }, 
        "resolver": {
            "nameservers": [
                "10.164.0.11", 
                "10.164.0.12"
            ], 
            "search": "staging.dmz"
        }
    }
}
